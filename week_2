//ques_1
#include<stdio.h>
#include<stdlib.h>

void binary_search(int arr[], int n , int key)
{
    int f = 1;
    int l = 3 ;
    int i , mid = 0 ;
    int flag = 1 ;
    
    while(f<l)
    {
        mid = (f+l)/2;
        if (arr[mid]==key)
        {
            printf("key found at %d \n",mid);
            flag = 0 ;
            break ;
        }
        else if ( arr[mid]> key)
        {
            l = mid - 1 ;
        }
        else if ( arr[mid]< key)
        {
            f = mid + 1 ;
        }
        if(flag)
        {
            printf("key not found\n");
        }
    } 
        
        printf("element %d occur %d times", key ,(l-f+1));
        return ( l - f + 1  );
}

int main()
{
    int n , i ;
    printf("Enter the number of elements:\n");
    scanf("%d",&n);
    int arr[n];
    printf("Enter the  number of elements:\n");
    for(i=0 ; i<n ; i++)
    {
        scanf("%d",&arr[i]);                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            
    }
    int key ;
    printf("Enter the key \n");
    scanf("%d",&key);
    
    binary_search(arr , n ,key);
}
//second
#include<stdio.h>
#include<stdlib.h>
void find_sequence(int arr[] , int i , int j , int  k )
{
 if(arr[i]+arr[j]==arr[k])
 {
     printf("Sequence Is Found \n");
 }
 else
 {
     printf("Sequence Is Not Found \n");
 }
 printf("%d + %d = %d \n", i , j , k);
}

int main()
{
    int n , m ;
    printf("Enter the number of elements:\n");
    scanf("%d",&n);
    int arr[n];
    printf("Enter the  number of elements:\n");
    for(m=0 ; m<n ; m++)
    {
        scanf("%d",&arr[m]);                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            
    }
   int i = 0 , j = 0 , k = 0 ;

   while(i<n)
   {
       for(i=0;i<n;i++)
       {
         j=j+1 ;
       }
      for(k=0;k<n;k++)
      {
       printf("key exist\n");
      }
   }   printf("Enter the value of first digit :\n");
   scanf("%d",&i);
   printf("Enter the value of second digit :\n");
   scanf("%d",&j);
  find_sequence(arr , i , j , k );
   
}
