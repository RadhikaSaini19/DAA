//ques_1
#include<stdio.h>
#include<stdlib.h>

int main()
{
int n , i , t , k = 0  , c = 0 ;
printf("enter the  number of test cases\n");
scanf("%d",&t);
while(k<t)
{
  printf("Enter the number of elements\n");
  scanf("%d",&n);
  int arr[n];
  printf("Enter the elemenst:\n");
  for(i=0;i<n;i++)
  {
    scanf("%d",&arr[i]);
  }
  int key ;
  printf("enter the key\n");
  scanf("%d",&key);
  for(i=0;i<n;i++)
  {
    c++;
    if(key==arr[i])
    {
    printf("number of comparisons are:\n");
    printf("key found\n");
    break;
    }
    }
    if(i==n)
    {
    printf("key not found");
    }
    k++;
    }
    }
